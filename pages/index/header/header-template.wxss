.header-container {
  width: 100%;
  height: 120rpx;
}

.header-container image {
  width: 100%;
  height: 120rpx;
  position: relative;
}

.header-container .logo {
  width: 56%;
  position: absolute;
  top: 0;
  left: 0;
  height: 120rpx;
}

.header-container button {
  width: 100%;
  height: 120rpx;
}

.header-container .tel {
  width: 44%;
  position: absolute;
  top: 0;
  right: 0;
  height: 120rpx;
}

.header-container .tel button {
  width: 100%;
  height: 120rpx;
}

.header-container .tel text {
  color: #0094aa;
  width: 100%;
  height: 120rpx;
}
