.dbswt {
  position: fixed;
  max-width: 750rpx;
  bottom: 0;
  width: 100%;
  height: 155rpx;
  z-index: 999;
}

.dbswt image {
  width: 100%;
  height: 155rpx;
  z-index: -1;
}

.dbswt .post {
  position: relative;
}

.dbswt .abs {
  position: absolute;
  bottom: 20rpx;
  left: 100rpx;
}

.dbswt .abs text {
  font-size: 26rpx;
  color:#fff;
}

.dbswt .abs button {
  float:left;
  margin-right: 100rpx;
  margin-top: 5rpx;
}

.dbswt .abs .foot-tel {
  float: left;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.dbswt .abs navigator {
  float: left;
  margin-left: 165rpx;
  line-height: 40rpx;
}